{
  "name": "artflow-studio/table",
  "description": "A custom package to render Livewire components using the @AFtable directive.",
  "autoload": {
    "psr-4": {
      "ArtflowStudio\\Table\\": "src/",
      "ArtflowStudio\\Table\\Database\\Seeders\\": "database/seeders/",
      "ArtflowStudio\\Table\\Tests\\Models\\": "tests/Models/"
    }
  },
  "require": {
    "php": "*",
    "livewire/livewire": "*",
    "illuminate/view": "*"
  },
  "extra": {
    "laravel": {
      "providers": [
        "ArtflowStudio\\Table\\TableServiceProvider"
      ]
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Artflow Studio",
      "email": "support@artflowstudio.com",
      "homepage": "https://artflowstudio.com",
      "role": "Developer"
    }
  ],
  "keywords": [
    "laravel",
    "livewire",
    "datatable",
    "table",
    "component",
    "artflow"
  ],
  "homepage": "https://github.com/artflow-studio/table",
  "support": {
    "issues": "https://github.com/artflow-studio/table/issues",
    "source": "https://github.com/artflow-studio/table"
  },
  "suggest": {
    "maatwebsite/excel": "For Excel export support",
    "barryvdh/laravel-snappy": "For PDF export support"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
