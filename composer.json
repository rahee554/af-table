  "autoload": {
    "psr-4": {
      "ArtflowStudio\\Table\\": "src/"
    }
  },
  "require": {
    "php": "^8.0",
    "livewire/livewire": "^3.5",
    "maatwebsite/excel": "^3.1",
    "illuminate/view": "^10.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "ArtflowStudio\\Table\\TableServiceProvider"
      ]
    }
  }
}
